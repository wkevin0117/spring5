<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="productA" class="lab2.ex2.Product"
		p:productId="10001"
		p:name="dog"
		p:description="A Dog---" />
	<bean id="productB" class="lab2.ex2.Product" scope="prototype">
		<property name="productId" value="10002" />
		<property name="name" value="cat" />
		<property name="description" value="A Cat" />
	</bean>
	<bean id="product" class="lab2.ex2.ProductCollection">
		<property name="list">
			<list>
				<ref bean="productA"/>
				<ref bean="productB"/>
			</list>
		</property>
	</bean>

</beans>
